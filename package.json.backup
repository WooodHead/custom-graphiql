    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-react": "6.7.0",
    "express": "^4.13.4",
    "flow-bin": "^0.35.0",
    "flow-status-webpack-plugin": "^0.1.7",
