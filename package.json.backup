    "tcomb": "^3.2.15",
    "webpack": "^1.12.2",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.10.0",
    "promise": "7.1.1",
    "object-assign": "4.1.0",
